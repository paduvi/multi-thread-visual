<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.FlowPane?>


<FlowPane alignment="CENTER" columnHalignment="CENTER" orientation="VERTICAL" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="com.chotoxautinh.controller.DemoSemaphoreController2">
   <children>
      <ProgressBar fx:id="progressBar1" prefWidth="500.0" progress="0.0">
         <FlowPane.margin>
            <Insets bottom="15.0" top="15.0" />
         </FlowPane.margin>
      </ProgressBar>
      <ProgressBar fx:id="progressBar2" prefWidth="500.0" progress="0.0" style="-fx-accent: green;">
         <FlowPane.margin>
            <Insets bottom="15.0" top="15.0" />
         </FlowPane.margin>
      </ProgressBar>
      <ProgressBar fx:id="progressBar3" prefWidth="500.0" progress="0.0" style="-fx-accent: pink;">
         <FlowPane.margin>
            <Insets bottom="15.0" top="15.0" />
         </FlowPane.margin>
      </ProgressBar>
      <Button fx:id="startBtn" defaultButton="true" mnemonicParsing="false" onAction="#start" text="Start">
         <FlowPane.margin>
            <Insets bottom="15.0" top="15.0" />
         </FlowPane.margin>
      </Button>
   </children>
</FlowPane>
