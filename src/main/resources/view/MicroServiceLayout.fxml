<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.FlowPane?>


<FlowPane alignment="CENTER" columnHalignment="CENTER" orientation="VERTICAL" prefHeight="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="com.chotoxautinh.controller.MicroServiceController">
   <children>
      <Label text="1 Big Task:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <ProgressBar fx:id="singleProgressBar" prefWidth="500.0" progress="0.0">
         <FlowPane.margin>
            <Insets bottom="15.0" top="15.0" />
         </FlowPane.margin>
      </ProgressBar>
      <Label text="4 Divided Tasks:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <ProgressBar fx:id="multiProgressBar" prefWidth="500.0" progress="0.0">
         <FlowPane.margin>
            <Insets bottom="15.0" top="15.0" />
         </FlowPane.margin>
      </ProgressBar>
      <Button fx:id="startBtn" defaultButton="true" mnemonicParsing="false" onAction="#start" text="Start">
         <FlowPane.margin>
            <Insets top="15.0" />
         </FlowPane.margin>
      </Button>
   </children>
</FlowPane>
