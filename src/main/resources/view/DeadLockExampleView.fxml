<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.FlowPane?>


<FlowPane alignment="CENTER" columnHalignment="CENTER" orientation="VERTICAL" prefHeight="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="com.chotoxautinh.controller.DeadLockExampleController">
   <children>
      <ImageView fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/slide_16.jpg" />
         </image>
      </ImageView>
      <ProgressBar fx:id="progressBar1" prefWidth="500.0" progress="0.0">
         <FlowPane.margin>
            <Insets bottom="15.0" top="15.0" />
         </FlowPane.margin>
      </ProgressBar>
      <ProgressBar fx:id="progressBar2" prefWidth="500.0" progress="0.0">
         <FlowPane.margin>
            <Insets bottom="15.0" top="15.0" />
         </FlowPane.margin>
      </ProgressBar>
      <HBox alignment="CENTER" fillHeight="false" spacing="30.0">
         <children>
            <Button fx:id="startBtn" defaultButton="true" mnemonicParsing="false" onAction="#start" text="Start" />
            <Button fx:id="stopBtn" disable="true" mnemonicParsing="false" onAction="#stop" text="Stop" />
         </children>
      </HBox>
   </children>
</FlowPane>
