      <HBox fx:id="sideBar" translateX="-350.0">
         <children>
            <ListView fx:id="listMenu" prefWidth="350.0" styleClass="menuList" />
            <VBox prefHeight="500.0" spacing="10.0" style="-fx-background-color: #4DB6AC;" BorderPane.alignment="CENTER">
