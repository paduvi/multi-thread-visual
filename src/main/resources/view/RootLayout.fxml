<AnchorPane maxHeight="500.0" minHeight="500.0" prefHeight="500.0" prefWidth="600.0" style="-fx-background-color: #DEDEDE;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chotoxautinh.controller.RootController">
